   "source": [
    "def countWays(x, n):\n",
    "    dp = [1] + [0] * x\n",
    "    for i in range(1, int(pow(x, 1 / n)) + 1):\n",
    "        u = i ** n\n",
    "        for j in range(x, u - 1, -1):\n",
    "            dp[j] += dp[j - u]\n",
    "    return(dp[-1])"
   ]
  },
